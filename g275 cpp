#include<bits/stdc++.h>
using namespace std;

int main(){
    int n;cin>>n;
    int a[7],b[7];
    for(int ii=0;ii<n;ii++){
        int wrong=0;
        for(int i=0;i<7;i++){
            cin>>a[i];
        }
        for(int i=0;i<7;i++){
            cin>>b[i];
        }
        if(a[1]==a[3]||a[1]!=a[5]||b[1]==b[3]||b[1]!=b[5]){
            cout<<"A";
            wrong++;
        }
        if(a[6]==0||b[6]==1){
            cout<<"B";
            wrong++;
        }
        if(a[1]==b[1]||a[3]==b[3]||a[5]==b[5]){
            cout<<"C";
            wrong++;
        }
        if(wrong==0)
            cout<<"None";
        cout<<"\n";
    }
}
