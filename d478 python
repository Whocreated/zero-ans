import sys

def solve():
    # 使用 sys.stdin.read().split() 一次讀取所有輸入，效率較高
    input_data = sys.stdin.read().split()
    if not input_data:
        return
    
    idx = 0
    n = int(input_data[idx])
    m = int(input_data[idx + 1])
    idx += 2
    
    for _ in range(n):
        # 讀取第一個數列
        list1 = input_data[idx : idx + m]
        idx += m
        # 讀取第二個數列
        list2 = input_data[idx : idx + m]
        idx += m
        
        # 將第二個數列轉為 set 以利快速查詢
        num_set = set(list2)
        
        # 計算 list1 中有多少元素出現在 num_set 中
        same_count = 0
        for val in list1:
            if val in num_set:
                same_count += 1
        
        print(same_count)

if __name__ == "__main__":
    solve()
