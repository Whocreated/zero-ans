#include <bits/stdc++.h>
using namespace std;
class c{
public:
    int atk;
    int def;
    int power(void){return this->atk*this->atk+this->def*this->def;}
};
bool cmp(c&a,c&b){return a.power()>b.power();}
int main(){
    int n;
    cin>>n;
    c*p=new c[n];
    for(int i=0;i<n;i++)cin>>p[i].atk>>p[i].def;
    sort(p,p+n,cmp);
    cout<<p[1].atk<<' '<<p[1].def<<'\n';
    return 0;
}
