#include <bits/stdc++.h>
using namespace std;
int main() {
    int a,b,c,r;double x1,x2,x;
    cin>>a>>b>>c;
    if (a!=0){
            r=b*b - 4*a*c;
            if(r>0){
                x1=(b*-1 + sqrt(r))/(2*a);
                x2=(b*-1 - sqrt(r))/(2*a);
                if(x1>x2)
                    cout<<"Two different roots x1="<<x1<<" , x2="<<x2;
                else
                    cout<<"Two different roots x1="<<x2<<" , x2="<<x1;
            }
            else if(r==0){
                x=(b*-1 + sqrt(r))/(2*a);
                cout<<"Two same roots x="<<x;
            }
            else
                cout<<"No real root";
    }
    else
        cout<<"No real root";
    return 0;
}
