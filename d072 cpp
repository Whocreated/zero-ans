#include <bits/stdc++.h>
int main(){
	int n;
	scanf("%d",&n);
	int a[n];
	for (int i = 0; i < n; i++){
		scanf("%d",&a[i]);
	}
	for (int i = 0; i < n; i++){
		printf("Case %d: ",i + 1);
		if (a[i] % 400 == 0 || (a[i] % 4 == 0 && a[i] % 100 != 0)){
			printf("a leap year\n");
		}else{
			printf("a normal year\n");
		}
	}
}
